{
    "compilerOptions": {
        "sourceMap": false,
        "target": "ESNext",
        "module": "NodeNext",
        "moduleResolution": "Node16",
        "removeComments": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "types": [
            "node",
            "@wdio/globals/types",
            "expect-webdriverio",
            "@wdio/cucumber-framework",
            "@wdio/browserstack-service",
            "@wdio/appium-service"
        ],
    },
}